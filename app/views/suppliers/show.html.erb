<div class="container mt-3">
  <div class="row">
    <div class="col-9">
      <h3 class="mt-3">Equipamento</h3>
    </div>
    <div class="col-3 text-end">
      <%= link_to "", new_supplier_path, class: 'mt-3 btn btn-outline-primary bi bi-plus' %>
    </div>
    <%= render @supplier %>

    <div>
      <%= link_to "Editar", edit_supplier_path(@supplier), class: "btn btn-primary" %>
      <%= link_to "Voltar", suppliers_path, class: "btn btn-primary" %>
      <%= button_to "Apagar", @supplier, method: :delete, class: "mt-2 btn btn-danger" %>
    </div>
  </div>
</div>



